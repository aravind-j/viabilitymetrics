@article{simoes_controlled_2008,
	title = {Controlled seed deterioration in \emph{{Tagetes} patula} {L}. cultivars},
	volume = {36},
	doi = {10.15258/sst.2008.36.3.02},
	abstract = {This work followed seed behaviour of Tagetes patula cultivars Spry, Orange, Flame and Yellow under controlled deterioration conditions and also established viability equation constants. Seed moisture contents (mc) were adjusted at 25°C from initial values (± 7.0\%), either by rehydration above water or by dehydration over silica gel. Seed sub samples were sealed in laminated aluminium-foil packets and stored in incubators maintained at 40, 50 and 65°C (± 0.5°C) until the complete survival curves were obtained. The species presented orthodox behaviour during storage. An inverse relationship between seed mc and longevity was detected. Different viability constant sets were estimated to predict seed longevity of the cultivars. The viability constant set calculated for the species T. patula was: KE = 12.22, CW = 3.114; CH = 0.2769, CQ = 0.002212.},
